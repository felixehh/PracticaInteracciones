<Window x:Class="PracticaInteracciones.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:PracticaInteracciones"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <TextBlock Margin="10,0,716,394">Cuenta</TextBlock>
        <TextBox Name="txtCuenta" Margin="10,25,592,362"></TextBox>
        <Slider ValueChanged="sldPrcPrp_ValueChanged" Name="sldPrcPrp" HorizontalAlignment="Left" Margin="10,67,0,0" VerticalAlignment="Top" RenderTransformOrigin="-11.5,-1.778" Width="190" Maximum="25"/>
        <TextBlock Name="lblPrcPrp" Margin="214,67,253,319">0%</TextBlock>
        <TextBlock HorizontalAlignment="Left" Margin="10,110,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="190"><Run Text="Total con Pro"/><Run Text="pinas"/></TextBlock>
        <TextBlock Name="lblTotalConPrp" HorizontalAlignment="Left" Margin="10,131,0,0" TextWrapping="Wrap" Text="$0.00" VerticalAlignment="Top" Height="29" Width="117"/>

    </Grid>
</Window>
